{
	"name": "spoticromos",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:qq": "node scripts/generate-qq-locale.js",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"clean": "rm -rf dist .svelte-kit",
		"import:playlist": "vite-node -c scripts/vite.scripts.config.ts scripts/import-playlist.ts --",
		"import:user": "vite-node -c scripts/vite.scripts.config.ts scripts/import-user-playlists.ts --"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^7.1.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"classnames": "^2.5.1",
		"mysql2": "^3.16.3",
		"svelte-i18n": "^4.0.1"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^4.0.6",
		"@vitest/ui": "^4.0.6",
		"daisyui": "^5.0.50",
		"eslint": "^9.18.0",
		"happy-dom": "^20.0.10",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vite-node": "^5.3.0",
		"vitest": "^4.0.6"
	}
}
